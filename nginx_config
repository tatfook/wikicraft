# Default server configuration
#
server {
        # listen 80 default_server;
        # listen [::]:80 default_server;

        # SSL configuration
        #
        # listen 443 ssl default_server;
        # listen [::]:443 ssl default_server;
        #
        # Self signed certs generated by the ssl-cert package
        # Don't use them in a production server!
        #
        # include snippets/snakeoil.conf;

        root /var/www/html;

        # Add index.php to the list if you are using PHP
        index index.html index.htm index.nginx-debian.html;

        server_name localhost;

        location / {
                # First attempt to serve request as file, then
                # as directory, then fall back to displaying a 404.
                try_files $uri $uri/ =404;
        }

        # pass the PHP scripts to FastCGI server listening on 127.0.0.1:9000
        #
        #location ~ \.php$ {
        #       include snippets/fastcgi-php.conf;
        #
        #       # With php5-cgi alone:
        #       fastcgi_pass 127.0.0.1:9000;
        #       # With php5-fpm:
        #       fastcgi_pass unix:/var/run/php5-fpm.sock;
        #}

        # deny access to .htaccess files, if Apache's document root
        # concurs with nginx's one
        #
        #location ~ /\.ht {
        #       deny all;
        #}
}

server {
        listen 80;
        server_name qiankunew.com *.qiankunew.com;
        location / {
                proxy_pass http://127.0.0.1:8088;
                include proxy_params;
        }
}

#server {
#       listen 80;
#       server_name gitlab.keepwork.com;
#       location / {
#               proxy_pass http://127.0.0.1:9000;
#               include proxy_params;
#       }
#}

server {
        listen 80;
        server_name dev.keepwork.com;

        location / {
                proxy_pass http://127.0.0.1:8900;
                include proxy_params;
        }
}

server {
        listen 80;
        server_name 47.90.1.144;

        location / {
                proxy_pass http://127.0.0.1:8099;
                include proxy_params;
        }
}

server {
        listen 80;
        server_name keepwork.com *.keepwork.com;

        if ($host = "www.keepwork.com") {
                rewrite ^(.*) http://keepwork.com$1 permanent;
        }

        location / {
                proxy_pass http://127.0.0.1:8099;
                include proxy_params;
        }
}


